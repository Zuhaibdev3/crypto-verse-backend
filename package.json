{
  "name": "backend-structure",
  "type": "commonjs",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/app.js",
  "scripts": {
    "start": "npm run build && npm run serve",
    "serve": "node -r dotenv/config dist/src/app.js",
    "build": "npm run clean && npm run build-ts && npm run keys",
    "build-local": "npm run clean && npm run build-ts",
    "build-ts": "tsc",
    "clean": "rimraf ./dist",
    "keys": "cp -r keys/ dist/",
    "start:dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only --exit-child --watch src ./src/app.ts",
    "start:prod": "cross-env NODE_ENV=production ts-node-dev --respawn --transpile-only --exit-child --watch src ./src/app.ts",
    "production": "node dist/src/app.js",
    "production:pm2": "pm2 start --name cryptoverse-backend dist/src/app.js",
    "seed:roles": "ts-node-dev src/seed/index.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "author": "Crypto-Verse Team",
  "license": "ISC",
  "dependencies": {
    "@types/eslint": "^8.4.10",
    "api": "^6.1.1",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "cloudinary": "^1.41.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.9",
    "eslint": "^8.30.0",
    "express": "^4.17.1",
    "firebase-admin": "^12.6.0",
    "helmet": "^4.6.0",
    "i": "^0.3.7",
    "inversify": "^6.0.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "lint": "^0.8.19",
    "lodash": "^4.17.21",
    "mongoose": "^6.0.8",
    "multer": "^1.4.5-lts.1",
    "npm": "^8.18.0",
    "openai": "^4.65.0",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cookie-session": "^2.0.44",
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/multer": "^1.4.12",
    "@types/node": "^16.10.1",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "cross-env": "^7.0.3",
    "joi-to-typescript": "^4.0.7",
    "nodemon": "^2.0.13",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.3.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3"
  }
}
